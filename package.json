{
  "name": "health",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" npm run payload:migrate && next build",
    "build:vercel": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" npm run payload:migrate && npm run generate:types && next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "payload:migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "payload:migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "payload:migrate:status": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:status",
    "payload:migrate:fresh": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@lexical/selection": "0.28.0",
    "@lexical/utils": "0.28.0",
    "@payloadcms/db-vercel-postgres": "3.50.0",
    "@payloadcms/next": "3.50.0",
    "@payloadcms/payload-cloud": "3.50.0",
    "@payloadcms/plugin-form-builder": "^3.50.0",
    "@payloadcms/plugin-search": "^3.50.0",
    "@payloadcms/plugin-seo": "^3.50.0",
    "@payloadcms/richtext-lexical": "3.50.0",
    "@payloadcms/storage-vercel-blob": "^3.50.0",
    "@payloadcms/ui": "3.50.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "lexical": "0.28.0",
    "lucide-react": "^0.469.0",
    "next": "15.3.2",
    "payload": "3.50.0",
    "react": "19.1.0",
    "react-icons": "^5.5.0",
    "sharp": "0.34.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "1.50.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "jsdom": "26.1.0",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "postcss": "^8.4.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "overrides": {
    "payload": "3.50.0",
    "@payloadcms/*": "3.50.0",
    "@lexical/*": "0.28.0",
    "lexical": "0.28.0"
  }
}
